{
	"transforms": [
		{
			"mode": "sql",
			"include": "**/*.sql",
			"emitTemplate": "{{dir}}/{{name}}.queries.ts"
		},
		{
			"mode": "ts",
			"include": "models/*.ts",
			"emitTemplate": "{{dir}}/{{name}}.types.ts"
		}
	],
	"srcDir": "./src/",
	"failOnError": false,
	"camelCaseColumnNames": false,
	"db": {
		"host": "localhost",
		"user": "postgres",
		"dbName": "stocks",
		"password": "password"
	}
}
